<% if @rows.present?  %>
<% @rows.each do |row| %>
 <div class="row-fluid">
  <ul class="thumbnails">
    <li class="span4">
      <div class="thumbnail">
        <%= row['url'] %>
        <div class="caption">
          <h3><%= row['title'] %> </h3>
          <p><%= row['description'] %></p>
          <p><a href="#" class="btn btn-primary">Action</a> <a href="#" class="btn">Action</a></p>
        </div>
      </div>
    </li>
  </ul>
</div>

<% end %>
<% end %>